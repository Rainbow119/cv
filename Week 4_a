#include <iostream>
using namespace std;

int main () {

    int inputNumber;
    cout<<"Please enter a positive integer: \n";
    cin>>inputNumber;

    int n = inputNumber;
    for (int i=1; i<=n; i++){
        for (int j=1; j<=n; j++){
            cout<<i * j<<'\t';
        }
        cout<<endl;
    }
    return 0;
}
